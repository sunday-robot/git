<Window x:Class="KuruKuru.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas x:Name="_GameCanvas" HorizontalAlignment="Left" Height="231" Margin="10,10,0,0" VerticalAlignment="Top" Width="300" MouseDown="_GameCanvas_MouseDown">
            <Ellipse x:Name="ellipse" Fill="#FFF4F4F5" Height="100" Canvas.Left="125" Stroke="Black" Canvas.Top="75" Width="100"/>
        </Canvas>

    </Grid>
</Window>
