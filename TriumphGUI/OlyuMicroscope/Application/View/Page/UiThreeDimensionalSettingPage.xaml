            <GradientStop Color="#FFCFCFCE" Offset="0.505618"/>
            <GradientStop Color="#FFE7E7E6" Offset="0.700086"/>
            <GradientStop Color="#FFFFFFFF" Offset="0.910112"/>
            <GradientStop Color="#FFB8B8B8" Offset="1"/>
