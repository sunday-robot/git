            <GradientStop Color="#FF575757" Offset="0.095506"/>
            <GradientStop Color="#FF393939" Offset="0.199648"/>
            <GradientStop Color="#FF1C1C1C" Offset="0.483146"/>
            <GradientStop Color="#FF2F2F2F" Offset="0.502697"/>
            <GradientStop Color="#FF424242" Offset="0.505618"/>
            <GradientStop Color="#FF7D7D7D" Offset="0.893258"/>
            <GradientStop Color="#FFB2B2B2" Offset="1"/>
