<Page
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:utility="clr-namespace:Olympus.LI.Triumph.Application.View.Utility"
    xmlns:CustomControl="clr-namespace:Olympus.LI.Triumph.Application.View.CustomControl"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d"
 	x:Class="Olympus.LI.Triumph.Application.View.UiDetailSettingPage"
	x:Name="Page"
	WindowTitle="Page"
	FlowDirection="LeftToRight" HorizontalAlignment="Center" VerticalAlignment="Top">
    
    <Page.Resources>
        <utility:EnumBooleanConverter x:Key="cnvEnumBoolean"/>
        <utility:EnumInt32Converter x:Key="cnvEnumInt32"/>
        <utility:EnumlistBooleanConverter x:Key="cnvEnumlistBoolean"/>
		<utility:EnumBoooleanConverterForToggle x:Key="cnvEnumBooleanToggle"/>
    </Page.Resources>

    <Grid Stylus.IsPressAndHoldEnabled="False">
        <Grid x:Name="grdZPosiotionSettingPage" Background="{DynamicResource AcquisitionNormalBackgroundBrush}" Visibility="{Binding sUiController.DetailSetting.SettingZPositionSettingPageVisibility, Mode=Default}">
			<StackPanel x:Name="stpZPosiotionSettingPage" Width="345" VerticalAlignment="Top" Margin="3,0,0,0">
                <Label x:Name="lblZPosiotionSettingTitle" HorizontalAlignment="Left" Foreground="White" FontSize="16"/>
                <Expander x:Name="expAFPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingAFPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingAFPageVisibility, Mode=Default}" Margin="0,5,0,0">
					<GroupBox x:Name="grbAF" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="120">
						<Grid Margin="0,5,0,0" IsEnabled="{Binding Path=sUiController.Focus.AFRoiSettingEnable, Mode=Default}">
                            <ToggleButton Width="100" x:Name="lblRoiAdj" IsChecked="{Binding sUiController.Focus.AFRoiSwitch, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top" Style="{DynamicResource ToggleButtonStyle}" Margin="10,0,0,0" Height="32"/>
                            <Button Width="100" x:Name="lblBtnDefault" IsEnabled="{Binding sUiController.Focus.AFRoiSwitch, Mode=Default}" Command="{Binding sUiController.Focus.AFRoiDefaultSetHandler, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="130,0,0,0" Style="{DynamicResource NormalButtonStyle}" Height="32"/>
                            <ToggleButton Width="100" x:Name="lblRoiDisp" IsChecked="{Binding sUiController.Focus.AFRoiDisplaySettingEnable, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top" Style="{DynamicResource ToggleButtonStyle}" Margin="10,45,0,0" Height="32"/>
                        </Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expZPositionRegistPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingZPositionRegistPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingZPositionRegistPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbZPositionRegist" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="100">
						<Grid d:LayoutOverrides="Height">
                            <CheckBox x:Name="lblCheckBoxZposAct" Margin="5,7,0,0" Height="18" Foreground="#FFFFFFFF" IsChecked="{Binding sUiController.Focus.ZHomePosEnable, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <Label Content="{Binding sUiController.Focus.ZHomePos, Mode=Default}" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,34,263,0" Foreground="White" />
                            <Label x:Name="lblZPositionUnit" Content="{Binding Path=sUiController.Layout.CommonUnit, Mode=Default}" Foreground="#FFFFFFFF" VerticalAlignment="Top" Margin="55,33,0,0" HorizontalAlignment="Left" />
                            <Button x:Name="lblBtnZposRegist" Command="{Binding sUiController.Focus.ZHomePosSetHandler, Mode=Default}" Style="{DynamicResource NormalButtonStyle}" VerticalAlignment="Top" Width="100" Height="32" Margin="150,33,0,0"  HorizontalAlignment="Left"/>
						</Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expLowerLimitRegistPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingLowerLimitRegistPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingLowerLimitRegistPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbLowerLimitRegist" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="100">
						<Grid>
							<CheckBox x:Name="lblCheckBoxLimitAct" Margin="5,7,0,0" Height="18" Foreground="#FFFFFFFF" IsChecked="{Binding sUiController.Focus.ZNearSampleLimitSwitch, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <Label x:Name="lblLimitAlerm" Margin="220,7,0,0" Visibility="{Binding sUiController.Focus.ZNearSampleLimitWarningVisible, Mode=Default}" Foreground="#FFFFFFFF" VerticalAlignment="Top" HorizontalAlignment="Left"/>
							<Label Content="{Binding sUiController.Focus.ZNearSampleLimitPos, Mode=Default}"  Foreground="#FFFFFFFF" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,34,263,0"/>
                            <Label x:Name="lblZPositionLimitUnit" Content="{Binding Path=sUiController.Layout.CommonUnit, Mode=Default}" Foreground="#FFFFFFFF" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="55,33,0,0" />
							<Button x:Name="lblBtnLowLmtregist" VerticalAlignment="Top" Command="{Binding sUiController.Focus.ZNearSampleLimitPosSetHandler, Mode=Default}" Style="{DynamicResource NormalButtonStyle}" Width="100" Height="32" Margin="150,33,0,0" HorizontalAlignment="Left"/>
						</Grid>
					</GroupBox>
				</Expander>
			</StackPanel>
		</Grid>
        <Grid x:Name="grdObservationSettingPage" Background="{DynamicResource AcquisitionNormalBackgroundBrush}" Visibility="{Binding sUiController.DetailSetting.SettingObservationSettingPageVisibility, Mode=Default}">
			<StackPanel x:Name="stpObservationSettingPage" Width="345" VerticalAlignment="Top" Margin="3,0,0,0">
                <Label x:Name="lblObservationSettingTitle" HorizontalAlignment="Left" Foreground="White" FontSize="16" Background="{x:Null}"/>
                <Expander x:Name="expBlurCorrectionPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingBlurCorrectionPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingBlurCorrectionPageVisibility, Mode=Default}" Margin="0,5,0,0">
					<GroupBox x:Name="grbBlurCorrection" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="65">
						<Grid>
							<CheckBox IsChecked="{Binding sUiController.Observation.BlurCorrectionOperation, Mode=Default}" Height="18" HorizontalAlignment="Left" Margin="5,3,0,0" x:Name="checkBox1" VerticalAlignment="Top" Width="17" />
							<Label Height="25" HorizontalAlignment="Left" Margin="25,-2,0,0" x:Name="label1" VerticalAlignment="Top" FontSize="13.333" Foreground="White" Padding="5,5,5,0" />
						</Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expASPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingASPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingASPageVisibility, Mode=Default}">
                    <GroupBox x:Name="grbAS" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="70">
                        <Grid IsEnabled="{Binding Path=sUiController.Microscope.ApertureStopEnable, Mode=Default}">
                            <ToggleButton x:Name="lblToggleBtnAS" IsChecked="{Binding sUiController.Microscope.ApertureStopState, ConverterParameter=IN, Converter={StaticResource cnvEnumBooleanToggle}, Mode=Default}" Height="32" Width="100" Margin="0,3,0,0" Style="{DynamicResource ToggleButtonStyle}" HorizontalAlignment="Center" VerticalAlignment="Top" />
                        </Grid>
                    </GroupBox>
                </Expander>
                <Expander x:Name="expAEPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsEnabled="{Binding sUiController.DetailSetting.SettingAESettingPageIsExpanded, Mode=Default}"  IsExpanded="{Binding sUiController.DetailSetting.SettingAEPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingAEPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbAE" BorderThickness="0.5" Foreground="White" Width="335" Height="145" VerticalAlignment="Top">
						<Grid Margin="0,5,0,0">
							<Label x:Name="lblAETarget" Content="Target" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" Margin="0,2,0,0"/>
							<Label Content="{Binding sUiController.Exposure.AETarget, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" VerticalAlignment="Top" Margin="80,2,0,0"/>
                            <Slider VerticalAlignment="Top" Maximum="{Binding sUiController.Exposure.AETargetMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.Exposure.AETargetMinimum, Mode=Default}" Value="{Binding sUiController.Exposure.AETarget, Mode=Default}" IsEnabled="{Binding sUiController.Exposure.AutoExposureEnable, Mode=Default}" Width="200" Style="{DynamicResource SliderStyle}" Margin="0" IsSnapToTickEnabled="True" HorizontalAlignment="Right" LargeChange="10"/>
                            <GroupBox x:Name="grbAERange" BorderThickness="0.5" Foreground="White" Width="320" Height="70" VerticalAlignment="Bottom" Margin="0,0,0,5" Header="範囲設定">
                            	<Grid>
                            		<ToggleButton x:Name="lblTglBtnAE" Width="100" Height="32" IsChecked="{Binding sUiController.Exposure.AERoiSwitch, Mode=Default}" IsEnabled="{Binding sUiController.Exposure.AEAreaAdjustmentEnable, Mode=Default}" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="30,2,0,0" Style="{DynamicResource ToggleButtonStyle}"/>
                                    <Button x:Name="lblBtnIniAE" Width="100" Height="32" IsEnabled="{Binding sUiController.Exposure.AEAreaDefaultEnable, Mode=Default}" Command="{Binding sUiController.Exposure.AERoiDefaultSetHandler, Mode=Default}" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,2,30,0" Style="{DynamicResource NormalButtonStyle}"/>
                            	</Grid>
                            </GroupBox>
						</Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expAspectPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingAspectPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingAspectPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbAspect" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="65">
                        <Grid Height="28" Margin="0" IsEnabled="{Binding Path=sUiController.Observation.ObservationAspectRatioIsEnable, Mode=Default}" HorizontalAlignment="Left">
							<RadioButton x:Name="lblRBtnAspect1to1" HorizontalAlignment="Left" Margin="5,0,0,0" Width="74" Height="18" Content="1:1" Foreground="#FFFFFFFF" IsChecked="{Binding sUiController.Observation.ObservationAspectRatio, ConverterParameter=AspectRatioNormal, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" Style="{DynamicResource RadioButtonStyle}" />
                            <RadioButton x:Name="lblRBtnAspect4to3" HorizontalAlignment="Left" Margin="100,0,0,0" Width="74" Height="18" Content="4:3" Foreground="#FFFFFFFF" IsChecked="{Binding sUiController.Observation.ObservationAspectRatio, ConverterParameter=AspectRatioWide, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" Style="{DynamicResource RadioButtonStyle}" />                                
						</Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expBinningPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingBinningPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingBinningPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbBinning" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="65">
		        		<ComboBox VerticalAlignment="Stretch" IsSynchronizedWithCurrentItem="True" SelectedIndex="{Binding sUiController.Observation.LiveBinningMode, Mode=Default}" Width="180" Height="30" Style="{DynamicResource ComboBoxStyle}">
		        			<ComboBoxItem x:Name="lblCombBoxOFF" Content="OFF"/>
                            <ComboBoxItem x:Name="lblCombBox2x2" Content="2x2"/>
		        		</ComboBox>
		        	</GroupBox>
				</Expander>
                <Expander x:Name="expMonochromeModePage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingMonochromeModePageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingMonochromeModePageVisibility, Mode=Default}">
                    <GroupBox x:Name="grbMonochromeMode" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="70">
                        <ToggleButton x:Name="lblToggleBtnMonochromeMode" IsChecked="{Binding Path=sUiController.ImageProcess.MonochromeModeEnable, Mode=Default}" Height="32" Width="100" Style="{DynamicResource ToggleButtonStyle}" IsEnabled="{Binding sUiController.ImageProcess.MonochromeModeStateEnable, Mode=Default}" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,2,0,0"/>
                    </GroupBox>
                </Expander>
            </StackPanel>
		</Grid>
        <Grid
            x:Name="grdMagnificationSettingPage" Background="{DynamicResource AcquisitionNormalBackgroundBrush}"
            Visibility="{Binding sUiController.DetailSetting.SettingMagnificationSettingPageVisibility, Mode=Default}">
            <StackPanel x:Name="stpMagnificationSettingPage" Width="345" VerticalAlignment="Top" Margin="3,0,0,0">
                <Label x:Name="lblMagnificationSettingTitle" HorizontalAlignment="Left" Foreground="White" FontSize="16" Background="{x:Null}"/>
                <Expander
                    x:Name="expMagnificationModeSettingPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" FontSize="13.333"
                    Visibility="{Binding sUiController.DetailSetting.SettingMagnificationModeSettingPageVisibility, Mode=Default}"
                    IsExpanded="{Binding sUiController.DetailSetting.SettingMagnificationModeSettingPageIsExpanded, Mode=Default}" Margin="0,5,0,0">
                    <GroupBox x:Name="grbMagnificationModeSetting" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top">
                        <StackPanel VerticalAlignment="Top" Margin="0,5,0,0" >
                            <RadioButton
                                x:Name="rbtnZoomMagnificationMode" HorizontalAlignment="Left" Margin="5,5,0,0" Height="21" Foreground="#FFFFFFFF"
                                IsChecked="{Binding sUiController.Magnification.MagnificationSpecifyMode, ConverterParameter=Zoom, Converter={StaticResource cnvEnumBoolean}, Mode=Default}"
                                Style="{DynamicResource RadioButtonStyle}"/>
                            <RadioButton
                                x:Name="rbtnTotalMagnificationMode" HorizontalAlignment="Left" Margin="5,5,0,0" Height="21" Foreground="#FFFFFFFF"
                                IsChecked="{Binding sUiController.Magnification.MagnificationSpecifyMode, ConverterParameter=Overall, Converter={StaticResource cnvEnumBoolean}, Mode=Default}"
                                Style="{DynamicResource RadioButtonStyle}"/>
                        </StackPanel>
                    </GroupBox>
                </Expander>
            </StackPanel>
        </Grid>
        <Grid x:Name="grdImageRevisionSettingPage" Background="{DynamicResource AcquisitionNormalBackgroundBrush}" Visibility="{Binding sUiController.DetailSetting.SettingImageRevisionSettingPageVisibility, Mode=Default}">
			<StackPanel x:Name="stpImageRevisionSettingPage" Width="345" VerticalAlignment="Top" Margin="3,0,0,0">
                <Label x:Name="lblImageRevisionSettingTitle" HorizontalAlignment="Left" Foreground="White" FontSize="16" Background="{x:Null}" Margin="-3,0,0,0"/>
                <Expander x:Name="expHDRPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingHDRPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingHDRPageVisibility, Mode=Default}" Margin="0,5,0,0">
		        	<GroupBox Width="335" Height="280" VerticalAlignment="Top" Margin="0,5,0,0" Foreground="White" BorderThickness="0.5" x:Name="grbHDR">
		        		<StackPanel Margin="5,0,0,0">
		        			<Grid x:Name="grdHDRAutoTuning" Height="37" VerticalAlignment="Top" Margin="0,8,0,0">
                                <CheckBox Content="マニュアル設定を固定する" IsChecked="{Binding sUiController.ImageProcess.HdrParamLock, Mode=Default}" Width="300" Height="25" VerticalAlignment="Top" Margin="8,0,0,0" FontSize="13" x:Name="tglbHDRParamLock" HorizontalAlignment="Left" d:LayoutOverrides="Width" Foreground="#FFFFFFFF" IsEnabled="{Binding Path=sUiController.ImageProcess.HdrParamLockEnable, Mode=Default}"/>
                            </Grid>
		        			<Grid x:Name="grdHDRBrightness" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrParamEnable, Mode=Default}" Margin="0,6,0,0">
                                <Label x:Name="lblHDRBrightness" FontSize="12" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Center" />
		        				<Label Content="{Binding sUiController.ImageProcess.HdrBrightness, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRBrightnessValue" Margin="98,0,0,0" VerticalAlignment="Center" />
                                <Slider Style="{DynamicResource SliderStyle}" Maximum="{Binding sUiController.ImageProcess.HdrBrightnessMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrBrightnessMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrBrightness, Mode=Default}" x:Name="sldHDRBrightness" HorizontalAlignment="Right" VerticalAlignment="Center" Width="172" Margin="0,0,8,0" IsSnapToTickEnabled="True" LargeChange="10"/>
		        			</Grid>
                            <Grid x:Name="grdHDRTexture" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrParamEnable, Mode=Default}" Margin="0,10,0,0">
		        				<Label x:Name="lblHDRTexture" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="White" FontSize="12" />
		        				<Label Content="{Binding sUiController.ImageProcess.HdrTexture, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRTextureValue" VerticalAlignment="Center" Margin="98,0,0,0" />
                                <Slider Style="{DynamicResource SliderStyle}" VerticalAlignment="Center" Maximum="{Binding sUiController.ImageProcess.HdrTextureMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrTextureMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrTexture, Mode=Default}" x:Name="sldHDRTexture" HorizontalAlignment="Right" Width="172" Margin="0,0,8,0" IsSnapToTickEnabled="True" LargeChange="10"/>
		        			</Grid>
                            <Grid x:Name="grdHDRContrast" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrParamEnable, Mode=Default}" Margin="0,10,0,0">
		        				<Label x:Name="lblHDRContrast" FontSize="12" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Center" />
		        				<Label Content="{Binding sUiController.ImageProcess.HdrContrast, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRContrastValue" VerticalAlignment="Center" Margin="98,0,0,0" />
                                <Slider Style="{DynamicResource SliderStyle}" Margin="0,0,8,0" VerticalAlignment="Center" Maximum="{Binding sUiController.ImageProcess.HdrContrastMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrContrastMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrContrast, Mode=Default}" x:Name="sldHDRContrast" Width="172" HorizontalAlignment="Right" IsSnapToTickEnabled="True" LargeChange="10"/>
		        			</Grid>
		        			<Grid x:Name="grdHDRChrome" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrParamEnable, Mode=Default}" Margin="0,10,0,0">
                                <Label x:Name="lblHDRChrome" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="12" Foreground="White" />
		        				<Label Content="{Binding sUiController.ImageProcess.HdrChrome, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRChromeValue" VerticalAlignment="Center" Margin="98,0,0,0" />
                                <Slider Style="{DynamicResource SliderStyle}" VerticalAlignment="Center" Maximum="{Binding sUiController.ImageProcess.HdrChromeMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrChromeMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrChrome, Mode=Default}" x:Name="sldHDRChrome" Width="172" HorizontalAlignment="Right" Margin="0,0,8,0" IsSnapToTickEnabled="True" LargeChange="10"/>
		        			</Grid>
		        		</StackPanel>
		        	</GroupBox>
				</Expander>
                <Expander x:Name="expContrastPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingContrastPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingContrastPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbContrast" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="65">
						<Grid Height="35" VerticalAlignment="Top" HorizontalAlignment="Left">
                            <RadioButton Style="{StaticResource RadioButtonStyle}" x:Name="lblRadioBtnL" Foreground="White" IsChecked="{Binding sUiController.ImageProcess.ContrastEmphasisMode, ConverterParameter=Low, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" IsEnabled="{Binding sUiController.ImageProcess.ContrastEmphasisEnable, Mode=Default}" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                            <RadioButton Style="{StaticResource RadioButtonStyle}" x:Name="lblRadioBtnM" Foreground="White" IsChecked="{Binding sUiController.ImageProcess.ContrastEmphasisMode, ConverterParameter=Normal, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" IsEnabled="{Binding sUiController.ImageProcess.ContrastEmphasisEnable, Mode=Default}" Margin="105,-1,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                            <RadioButton Style="{StaticResource RadioButtonStyle}" x:Name="lblRadioBtnH" Foreground="White" IsChecked="{Binding sUiController.ImageProcess.ContrastEmphasisMode, ConverterParameter=High, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" IsEnabled="{Binding sUiController.ImageProcess.ContrastEmphasisEnable, Mode=Default}" Margin="215,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
						</Grid>
					</GroupBox>
				</Expander>
                <Expander x:Name="expAntiHalationStandardPage" Foreground="White" Background="{x:Null}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="345" Margin="0,5,0,0" FontSize="13.333" IsExpanded="{Binding sUiController.DetailSetting.SettingAntiHalationStandardPageIsExpanded, Mode=Default}" Visibility="{Binding sUiController.DetailSetting.SettingAntiHalationStandardPageVisibility, Mode=Default}">
					<GroupBox x:Name="grbAntiHalationStandard" BorderThickness="0.5" Foreground="White" Width="335" VerticalAlignment="Top" Height="280">
						<StackPanel Margin="5,0,0,0">
                            <Grid x:Name="grdHDRAntiHalationAutoTuning" Height="37" VerticalAlignment="Top" Margin="0,8,0,0">
                                <CheckBox Content="マニュアル設定を固定する" IsChecked="{Binding sUiController.ImageProcess.HdrAntiHalationParamLock, Mode=Default}" Width="300" Height="25" VerticalAlignment="Top" Margin="8,0,0,0" FontSize="13" x:Name="tglbHDRAntiHalationParamLock" HorizontalAlignment="Left" d:LayoutOverrides="Width" Foreground="#FFFFFFFF" IsEnabled="{Binding Path=sUiController.ImageProcess.HdrAntiHalationParamLockEnable, Mode=Default}"/>
                            </Grid>
                            <Grid x:Name="grdHDRAntiHalationBrightness" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrAntiHalationParamEnable, Mode=Default}" Margin="0,6,0,0">
                                <Label x:Name="lblHDRAntiHalationBrightness" FontSize="12" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Center" />
                                <Label Content="{Binding sUiController.ImageProcess.HdrAntiHalationBrightness, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRAntiHalationBrightnessValue" Margin="98,0,0,0" VerticalAlignment="Center" />
                                <Slider Style="{DynamicResource SliderStyle}" Maximum="{Binding sUiController.ImageProcess.HdrBrightnessMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrBrightnessMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrAntiHalationBrightness, Mode=Default}" x:Name="sldHDRAntiHalationBrightness" HorizontalAlignment="Right" VerticalAlignment="Center" Width="172" Margin="0,0,8,0" IsSnapToTickEnabled="True" LargeChange="10"/>
							</Grid>
                            <Grid x:Name="grdHDRAntiHalationTexture" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrAntiHalationParamEnable, Mode=Default}" Margin="0,10,0,0">
                                <Label x:Name="lblHDRAntiHalationTexture" HorizontalAlignment="Left" VerticalAlignment="Center" Foreground="White" FontSize="12" />
                                <Label Content="{Binding sUiController.ImageProcess.HdrAntiHalationTexture, Mode=Default}" HorizontalAlignment="Left" Foreground="#FFFFFFFF" x:Name="lblHDRAntiHalationTextureValue" VerticalAlignment="Center" Margin="98,0,0,0" />
                                <Slider Style="{DynamicResource SliderStyle}" VerticalAlignment="Center" Maximum="{Binding sUiController.ImageProcess.HdrTextureMaximum, Mode=Default}" SmallChange="1" Minimum="{Binding sUiController.ImageProcess.HdrTextureMinimum, Mode=Default}" Value="{Binding sUiController.ImageProcess.HdrAntiHalationTexture, Mode=Default}" x:Name="sldHDRAntiHalationTexture" HorizontalAlignment="Right" Width="172" Margin="0,0,8,0" IsSnapToTickEnabled="True" LargeChange="10"/>
							</Grid>
                            <Grid x:Name="grdHDRAntiHalationContrast" Height="37" VerticalAlignment="Top" IsEnabled="{Binding sUiController.ImageProcess.HdrAntiHalationParamEnable, Mode=Default}" Margin="0,10,0,0">
