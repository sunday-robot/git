								</Setter.Value>
							</Setter>
							<Setter Property="Foreground" Value="#FFFFFFFF"/>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource OverStrokeBrush}"/>
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource NomalButtonBackgroundBrush}"/>
							<Setter Property="Fill" TargetName="RectangleFilter" Value="{x:Null}"/>
							<Setter Property="Stroke" TargetName="RectangleFilter" Value="{x:Null}"/>
