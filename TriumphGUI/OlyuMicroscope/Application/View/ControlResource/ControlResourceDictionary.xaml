	<Style x:Key="SimpleButtonStyle" TargetType="{x:Type Button}">
		<Setter Property="FocusVisualStyle" Value="{StaticResource ButtonFocusVisual}"/>
		<Setter Property="Background" Value="{StaticResource ButtonNormalBackground}"/>
		<Setter Property="BorderBrush" Value="{StaticResource ButtonNormalBorder}"/>
		<Setter Property="BorderThickness" Value="1"/>
		<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
		<Setter Property="HorizontalContentAlignment" Value="Center"/>
		<Setter Property="VerticalContentAlignment" Value="Center"/>
		<Setter Property="Padding" Value="1"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type Button}">
					<Grid Width="Auto" Height="Auto" x:Name="grid">
						<Rectangle x:Name="rectangle" HorizontalAlignment="Stretch" Height="Auto" Stroke="{DynamicResource NormalStrokeBrush}" StrokeThickness="1.33333" VerticalAlignment="Stretch" Width="Auto" RadiusX="3" RadiusY="3" Fill="{DynamicResource SimpleButtonNormalBackgroundBrush}">
							<Rectangle.Effect>
								<DropShadowEffect BlurRadius="15"/>
							</Rectangle.Effect>
						</Rectangle>
						<ContentPresenter x:Name="contentPresenter" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" HorizontalAlignment="Center" VerticalAlignment="Center" RecognizesAccessKey="True"/>
						<Rectangle x:Name="RectangleFilter" HorizontalAlignment="Stretch" Height="Auto" Stroke="{x:Null}" StrokeThickness="1.33333" VerticalAlignment="Stretch" Width="Auto" RadiusX="3" RadiusY="3" Fill="{x:Null}"/>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="False">
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource NormalStrokeBrush}"/>
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource SimpleButtonNormalBackgroundBrush}"/>
							<Setter Property="Effect" TargetName="rectangle">
								<Setter.Value>
									<DropShadowEffect BlurRadius="15" Color="Black" Direction="315" ShadowDepth="5"/>
								</Setter.Value>
							</Setter>
							<Setter Property="Stroke" TargetName="RectangleFilter" Value="{x:Null}"/>
							<Setter Property="Fill" TargetName="RectangleFilter" Value="{x:Null}"/>
						</Trigger>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource SimpleButtonNormalBackgroundBrush}"/>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource OverStrokeBrush}"/>
							<Setter Property="Effect" TargetName="rectangle">
								<Setter.Value>
									<DropShadowEffect BlurRadius="15" Direction="0" ShadowDepth="0" Color="{DynamicResource OverBrillianceColor}"/>
								</Setter.Value>
							</Setter>
							<Setter Property="Fill" TargetName="RectangleFilter" Value="{x:Null}"/>
							<Setter Property="Stroke" TargetName="RectangleFilter" Value="{x:Null}"/>
						</Trigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="True"/>
								<Condition Property="IsPressed" Value="True"/>
							</MultiTrigger.Conditions>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource NormalStrokeBrush}"/>
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource SimpleButtonNormalBackgroundBrush}"/>
							<Setter Property="Effect" TargetName="rectangle">
								<Setter.Value>
									<DropShadowEffect BlurRadius="0" Direction="0" ShadowDepth="0"/>
								</Setter.Value>
							</Setter>
							<Setter Property="Margin" TargetName="contentPresenter" Value="1,1,0,0"/>
							<Setter Property="Stroke" TargetName="RectangleFilter" Value="{DynamicResource NormalStrokeBrush}"/>
							<Setter Property="Fill" TargetName="RectangleFilter" Value="{DynamicResource PressBackgroundBrush}"/>
						</MultiTrigger>
						<Trigger Property="IsEnabled" Value="False">
							<Setter Property="Fill" TargetName="rectangle" Value="{DynamicResource SimpleButtonNormalBackgroundBrush}"/>
							<Setter Property="Stroke" TargetName="rectangle" Value="{DynamicResource NormalStrokeBrush}"/>
							<Setter Property="Effect" TargetName="rectangle">
								<Setter.Value>
									<DropShadowEffect BlurRadius="0" Direction="0" ShadowDepth="0"/>
								</Setter.Value>
							</Setter>
							<Setter Property="Fill" TargetName="RectangleFilter" Value="{DynamicResource DisableBackgroundBrush}"/>
							<Setter Property="Stroke" TargetName="RectangleFilter" Value="{DynamicResource DisableStrokeBrush}"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
