        <Grid Height="540" Width="380" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,25,0,0" IsEnabled="{Binding sUiController.Acquisition.IsStitchingAreaSettings, Mode=Default}">
            <Path x:Name="pthSetting" Stretch="Fill" StrokeMiterLimit="2.75" Stroke="#FFA4A4A4" Data="F1 M 1259.51,946.429C 1259.51,948.052 1257.67,949.372 1255.4,949.372L 918.904,949.372C 916.64,949.372 914.803,948.052 914.803,946.429L 914.803,698.667C 914.803,697.042 916.64,695.725 918.905,695.725L 1255.4,695.725C 1257.67,695.725 1259.51,697.042 1259.51,698.667L 1259.51,946.429 Z " HorizontalAlignment="Center" VerticalAlignment="Center" Width="360" Height="520"/>
            <StackPanel x:Name="stpAutoSave2" Width="360" Height="520">
                <!--「完了後に貼り合わせ処理を実行する」チェックボックス-->
                <CheckBox Margin="15,15,0,0" x:Name="cbCompletedExecuteStitching" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IsCompletedExecuteStitchingChecked, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <!--「自動保存を有効にする」チェックボックス-->
                <Grid>
                    <CheckBox Content="{Binding sUiController.Stage.StageSettingDialogAutoSaveChkBox, Mode=Default}" HorizontalAlignment="Left" Margin="15,5,0,0" VerticalAlignment="Top" IsChecked="{Binding sUiController.Acquisition.AutoSaveCheck, Mode=Default}" />
                    <Button Content="{Binding sUiController.Stage.StageSettingDialogSelectSaveTo, Mode=Default}" Margin="214,2,0,0" Height="28" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="13.333" Command="{Binding sUiController.DetailSetting.ShowDetailSetteingAutoSavePageHandler, Mode=Default}" IsEnabled="{Binding sUiController.Acquisition.AutoSaveCheck, Mode=Default}" Style="{DynamicResource SimpleButtonStyle}" Width="120"/>
                </Grid>
                <!--「個別画像の自動保存を有効にする」チェックボックス-->
                <CheckBox Margin="15,5,0,0" x:Name="cbIndividuallyAutoSave" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IsIndividuallyAutoSaveChecked, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <GroupBox x:Name="grbIndividuallyAutoSave" Header="IndividuallyAutoSaveaaaaaaaaaaaaaaaaaaa" Margin="20,5,0,0" BorderThickness="0.5" Foreground="#00FFFFFF" Width="330" Height="410" BorderBrush="#FFA4A4A4" IsEnabled="{Binding sUiController.Acquisition.IsIndividuallyAutoSaveChecked, Mode=Default}">
                    <StackPanel x:Name="stpAutoSave">
                        <!--「個別画像の自動保存詳細を設定する」チェックボックス-->
                        <CheckBox Margin="5,-17,0,0" x:Name="cbIndividuallyAutoSaveDetailEnabled" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IsIndividuallyAutoSaveDetailChecked, Mode=Default}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <GroupBox x:Name="grbIndividuallyAutoSavePlace" Header="{Binding sUiController.Acquisition.AutoSavePlaceLabel, Mode=Default}" BorderThickness="0.5" BorderBrush="#FFA4A4A4" Foreground="Black" Width="310" VerticalAlignment="Top" Height="55" Margin="4,5,4,0" IsEnabled="{Binding sUiController.Acquisition.IndividuallyAutoSavePlaceEnable, Mode=Default}">
                        	<Grid x:Name="grdAutoSavePlace1" HorizontalAlignment="Stretch" VerticalAlignment="Top">
                                <RadioButton HorizontalAlignment="Left" Content="{Binding sUiController.Acquisition.AutoSavePlaceFileLabel, Mode=Default}" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IndividuallyAutoSavePlace, ConverterParameter=FILE, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" Style="{DynamicResource RadioButtonStyle}" Margin="5,5,0,0"/>
                                <RadioButton HorizontalAlignment="Center" Content="{Binding sUiController.Acquisition.AutoSavePlaceDBLabel, Mode=Default}" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IndividuallyAutoSavePlace, ConverterParameter=DB, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" Style="{DynamicResource RadioButtonStyle}" Margin="0,5,65,0" />
                                <RadioButton HorizontalAlignment="Right" Content="{Binding sUiController.Acquisition.AutoSavePlaceBothLabel, Mode=Default}" Foreground="Black" IsChecked="{Binding sUiController.Acquisition.IndividuallyAutoSavePlace, ConverterParameter=BOTH, Converter={StaticResource cnvEnumBoolean}, Mode=Default}" Style="{DynamicResource RadioButtonStyle}" Margin="0,5,45,0"/>
                        	</Grid>
                        </GroupBox>
                        <!--ファイル設定グループボックス-->
                        <GroupBox x:Name="grbIndividuallyFileSetting" Header="{Binding sUiController.Acquisition.AutoSaveFileSettingLabel, Mode=Default}" BorderThickness="0.5" BorderBrush="#FFA4A4A4" Foreground="Black" Width="310" VerticalAlignment="Top" Height="185" Margin="4,5,4,0" IsEnabled="{Binding sUiController.Acquisition.IndividuallyAutoSaveFileSettingEnable, Mode=Default}">
                    		<StackPanel x:Name="stpFileSetting1" Height="293" VerticalAlignment="Top">
                                <StackPanel x:Name="stpFolderPath1" IsEnabled="{Binding sUiController.Acquisition.IndividuallyAutoSaveFolderPathEnable, Mode=Default}">
                                    <!--フォルダ-->
                                    <Label x:Name="lblFolderPath1" HorizontalAlignment="Left" Content="{Binding sUiController.Acquisition.AutoSaveFolderPathLabel, Mode=Default}" Foreground="Black" Background="{x:Null}" Margin="5,0,0,0"/>
                                    <Grid x:Name="grdFolderPath1" HorizontalAlignment="Stretch" VerticalAlignment="Top">
                                        <Olympus_LI_Triumph_Application_View_CustomControl:ValidateTextBox InputType="String" FontSize="13" Text="{Binding sUiController.Acquisition.IndividuallyAutoSaveFolderPath, Mode=Default}" HorizontalAlignment="Left" Width="190" Margin="5,-2,0,2" MaxLines="1" Style="{DynamicResource TextBoxStyle}" VerticalAlignment="Center"/>
                                        <Button Width="70" Content="{Binding sUiController.Acquisition.AutoSaveReferLabel, Mode=Default}" Command="{Binding sUiController.Acquisition.IndividuallyAutoSaveFolderDialogShowHandler, Mode=Default}" HorizontalAlignment="Right" Style="{DynamicResource SimpleButtonStyle}" Margin="0,-5,13,0" Height="32" VerticalAlignment="Center"/>
                                    </Grid>
                                </StackPanel>
                                <!--ファイル名-->
                    			<Label x:Name="lblFileName1" HorizontalAlignment="Left" Content="{Binding sUiController.Acquisition.AutoSaveFileNameLabel, Mode=Default}" Foreground="Black" Background="{x:Null}" Margin="0"/>
                                <Olympus_LI_Triumph_Application_View_CustomControl:ValidateTextBox InputType="String" FontSize="13" Text="{Binding sUiController.Acquisition.IndividuallyAutoSaveFileName, Mode=Default}" HorizontalAlignment="Left" Width="190" Margin="5,-5,0,0" Style="{DynamicResource TextBoxStyle}" MaxLength="36"/>
                    			<Label x:Name="lblFileCounter1" HorizontalAlignment="Left" Content="{Binding sUiController.Acquisition.AutoSaveFileCounterLabel, Mode=Default}" Foreground="Black" Background="{x:Null}" Margin="0"/>
                                <!--ファイルカウンタ-->
                    			<Grid HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Margin="0,-5,0,0">
                                    <Olympus_LI_Triumph_Application_View_CustomControl:ValidateTextBox InputType="NumericPositive" InputMethod.IsInputMethodEnabled="False" FontSize="13" HorizontalAlignment="Left" Width="69.936" Height="26" Text="{Binding sUiController.Acquisition.IndividuallyAutoSaveFileCounter, Mode=Default}" VerticalAlignment="Top" Style="{DynamicResource TextBoxStyle}" Margin="5,0,0,0" TextAlignment="Right" MaxLength="4" DefaultSetReason="NumericParseError" DefaultValue="0" RangeMin="{Binding sUiController.Acquisition.AutoSaveFileCounterRangeMin, Mode=Default}" RangeMax="{Binding sUiController.Acquisition.AutoSaveFileCounterRangeMax, Mode=Default}" IsRangeCheck="True"/>
                                    <RepeatButton HorizontalAlignment="Left" Width="17.5" Height="13" FontSize="8" Foreground="{DynamicResource TextBoxButtonBrush}" Delay="500" Interval="1" Margin="75,0,0,0" VerticalAlignment="Top"    Command="{Binding sUiController.Acquisition.IndividuallyAutoSaveFileCounterIncreaseHandler, Mode=Default}" Style="{DynamicResource NumericUpButtonStyle}"/>
                                    <RepeatButton HorizontalAlignment="Left" Width="17.5" Height="13" FontSize="8" Foreground="{DynamicResource TextBoxButtonBrush}" Delay="500" Interval="1" Margin="75,0,0,0"  VerticalAlignment="Bottom" Command="{Binding sUiController.Acquisition.IndividuallyAutoSaveFileCounterDecreaseHandler, Mode=Default}" Style="{DynamicResource NumericDownButtonStyle}"/>
                    			</Grid>
                    		</StackPanel>
                    	</GroupBox>
                        <!--コメントグループボックス-->
                        <GroupBox x:Name="grbIndividuallyComment" Header="{Binding sUiController.Acquisition.AutoSaveCommentLabel, Mode=Default}" BorderThickness="0.5" BorderBrush="#FFA4A4A4" Foreground="Black" Width="310" VerticalAlignment="Top" Height="125" Margin="4,5,4,0" IsEnabled="{Binding sUiController.Acquisition.IndividuallyAutoSaveCommentEnable, Mode=Default}">
                            <Olympus_LI_Triumph_Application_View_CustomControl:ValidateTextBox InputType="String" FontSize="13" Style="{DynamicResource TextBoxStyle}" Text="{Binding sUiController.Acquisition.IndividuallyAutoSaveComment, Mode=Default}" HorizontalAlignment="Left" Width="285" Height="90" Margin="5,0,5,0" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" AcceptsReturn="True" MaxLength="40"/>
                    	</GroupBox>
                    </StackPanel>
                </GroupBox>
            </StackPanel>
        </Grid>
